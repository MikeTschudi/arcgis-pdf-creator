{
  "name": "arcgis-pdf-creator",
  "version": "0.0.1",
  "description": "Functions for multilingual writing into a PDF file from JavaScript",
  "scripts": {
    "build": "build.bat",
    "lint": "eslint --c test/.eslintrc.js --ext .ts src/**/*.ts",
    "lint:fix": "eslint --c test/.eslintrc.js --ext .ts --fix src/**/*.ts",
    "test": "start cmd.exe /q/c \"del/q node_modules\\jspdf\\dist\\jspdf.es.min.js 2>nul\" && karma start test\\karma.conf.js --single-run",
    "docs:build": "typedoc",
    "docs:deploy": "node support/deploy-doc-site.js"
  },
  "author": "Esri",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:MikeTschudi/arcgis-pdf-creator.git"
  },
  "devDependencies": {
    "@chiragrupani/karma-chromium-edge-launcher": "^2.2.1",
    "@types/jasmine": "^4.0.3",
    "@types/node": "^17.0.33",
    "@types/sinon": "^10.0.11",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/eslint-plugin-tslint": "^5.23.0",
    "@typescript-eslint/parser": "^5.25.0",
    "codecov": "^3.8.2",
    "coveralls": "^3.1.1",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.2.9",
    "eslint-plugin-n": "^15.2.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-promise": "^6.0.0",
    "gh-pages": "^4.0.0",
    "karma": "^6.3.20",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage": "^2.2.0",
    "karma-firefox-launcher": "^2.1.2",
    "karma-jasmine": "^5.0.1",
    "karma-jasmine-diff-reporter": "^2.0.1",
    "karma-requirejs": "^1.1.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-spec-reporter": "0.0.34",
    "karma-typescript": "^5.5.3",
    "karma-typescript-es6-transform": "^5.5.3",
    "prettier": "^2.6.2",
    "tslint": "^6.1.3",
    "typedoc": "^0.22.15",
